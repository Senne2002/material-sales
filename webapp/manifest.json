{
  "_version": "1.59.0",
  "sap.app": {
    "id": "ap.materialsales",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.5",
      "toolsId": "71dabc98-ffdf-4795-a84f-c6ce8471ef55"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSD_014_GW_MAT_SALES_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ap.materialsales.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "ap.materialsales.view",
        "controlId": "fcl",
        "transition": "slide",
        "bypassed": {},
        "async": true,
        "clearControlAggregation": false
      },
      "routes": [
        {
          "pattern": ":?layout:",
          "name": "master",
          "target": ["master"]
        },
        {
          "pattern": "detail/{material}/{layout}",
          "name": "detail",
          "target": ["master", "detail"]
        },
        {
          "pattern": "sales",
          "name": "sales",
          "target": ["sales"]
        }
      ],
      "targets": {
        "master": {
          "viewName": "Material",
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewName": "MaterialDetail",
          "controlAggregation": "midColumnPages"
        },
        "sales": {
          "viewName": "Sales",
          "controlAggregation": "beginColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "ap.materialsales.view.App",
      "type": "XML",
      "async": true,
      "id": "fcl"
    }
  }
}
